    /* Puedes ajustar este valor según sea necesario */
    left: -80px;
  }

  .pdf {
    position: relative;
    top: 30px;
    /* Puedes ajustar este valor según sea necesario */
    left: -55px;
  }
</style>
<script>
  function numberWithCommas(number) {
    var parts = number.toString().split(".");
    parts[0] = parts[0].replace(/\B(?=(\d{3})+(?!\d))/g, ",");
    return parts.join(".");
  }
  </script>