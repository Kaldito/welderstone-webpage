    .custom-select {
      appearance: none;
      background-color: #ffffff;
      border: 1px solid #cccccc;
      border-radius: 5px;
      color: #555555;
      font-size: 16px;
      padding: 10px;
      width: 100%;
    }
    
    .custom-select:focus {
      outline: none;
      border-color: #2196F3;
      box-shadow: 0 0 0 2px rgba(33, 150, 243, 0.2);
    }
    
    .custom-select option {
      background-color: #ffffff;
      color: #555555;
    }
    
